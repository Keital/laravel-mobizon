{
    "name": "alitvinov/laravel-mobizon",
    "description": "Mobizon SMS notifications driver for Laravel",
    "keywords": [
        "laravel",
        "notifications",
        "mobizon",
        "sms",
        "php"
    ],
    "homepage": "https://github.com/Alitvinov/mobizon",
    "license": "MIT",
    "authors": [
        {
            "name": "Alex Litvinov",
            "homepage": "https://github.com/Alitvinov",
            "role": "Developer"
        }
    ],
    "require": {
        "lib-curl": "*",
        "php": "^7.2.5|^8.0|^8.1",
        "ext-json": "*",
        "ext-mbstring": "*",
        "guzzlehttp/guzzle": "^6.3",
        "illuminate/queue": "5.1 - 5.8|^6.0|^7.0",
        "illuminate/notifications": "5.1 - 5.8|^6.0|^7.0",
        "illuminate/support": "5.1 - 5.8|^6.0|^7.0",
        "mobizon/mobizon-php": "~2.0"
        
    },
    "require-dev": {
        "mockery/mockery": "^0.9.5",
        "phpunit/phpunit": "4.*"
    },
    "autoload": {
        "psr-4": {
            "Alitvinov\\LaravelMobizon\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Alitvinov\\LaravelMobizon\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true
    }
}
